{
    "name": "algorithms-unlocked",
    "version": "1.0.0",
    "description": "Algorithms from 'Algorithms Unlocked' book",
    "main": "binary-search.js",
    "scripts": {
        "prepare": "npm i",
        "test": "jest",
        "watch:test": "jest --watchAll"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@types/jest": "^15.1.32",
        "jest-cli": "^15.1.1",
        "typescript": "^2.0.3"
    },
    "jest": {
        "scriptPreprocessor": "preprocessor.js",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js"
        ],
        "testPathDirs": [
            "src/"
        ],
        "testRegex": "(/__tests__/.*|\\.(test|spec))\\.ts$",
        "testEnvironment": "node",
        "coverageDirectory": "./coverage",
        "collectCoverage": true,
        "collectCoverageFrom": [
            "src/**/*.ts"
        ]
    }
}
